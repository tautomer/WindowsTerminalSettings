            "keys": "ctrl+shift+3"
        },
        {
            "command": "newTabProfile3",
            "keys": "ctrl+shift+4"
        },
        {
            "command": "nextTab",
            "keys": "ctrl+tab"
        },
        {
            "command": "openSettings",
            "keys": "ctrl+alt+,"
        },
        {
            "command": "prevTab",
            "keys": "ctrl+shift+tab"
        },
        {
            "command": "scrollDown",
            "keys": "ctrl+shift+down"
        },
        {
            "command": "scrollDownPage",
            "keys": "ctrl+shift+pgdn"
        },
        {
            "command": "scrollUp",
            "keys": "ctrl+shift+up"
        },
        {
            "command": "scrollUpPage",
            "keys": "ctrl+shift+pgup"
        },
        {
